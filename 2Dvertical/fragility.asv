function [ FragPoints ] = fragility( DPM, window, noLSs )
%UNTITLED2 Summary of this function goes here
%   Detailed explanation goes here

for i = 1:length(window)
	for j = 1: noLSs
		FragPoints(i,j) = sum(DPM(i,j:noLSs))	
	end
end
plot(window,FragPoints(:,2)%,'bc');
hold on
plot(window,FragPoints(:,3)%,'rc');
grid on

end

