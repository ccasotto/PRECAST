function stats=leastSquaresCloRes(IMLs,cumDamageStates,noLSs)

    stats=zeros(noLSs-1,3);  
    solutions=[4.5 0.1;5.5 0.05];
    for LS=1:(noLSs-1)
        f = @(x)closestResiduals(x,IMLs,cumDamageStates(:,LS+1))
        [x]=lsqnonlin(f,[solutions(LS,:)]);
        stats(LS,1:2)=x;
        stats(LS,3)=corr(IMLs,cumDamageStates(:,LS+1)).^2;

    end
end
